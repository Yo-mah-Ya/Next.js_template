{
  "name": "with-typescript",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p 3000",
